  },
  "devDependencies": {
    "colors-browserify": "~0.0.8",
    "chai-performance": "~0.1.2"
  },
  "main": "./lib",
  "browser": {
    "./lib/base/iterator/": "./lib/base/iterator/browser.js",
    "./lib/util/uuid/": "./lib/util/uuid/browser.js"
